{
  "name": "react-typeaheadix",
  "description": "A react typeahead component",
  "author": "Michael Wibmer <michael@pmpkin.com>",
  "version": "0.0.1",
  "main": "lib/index.js",
  "scripts": {
    "build:components": "./node_modules/.bin/webpack",
    "build:js": "babel src/ -d lib/ --ignore src/components",
    "build": "npm run clean && npm run build:components && npm run build:js",
    "clean": "rimraf lib",
    "lint": "eslint --ext .js,.jsx src test",
    "test": "./node_modules/.bin/_mocha ./test/**/*.spec.js --reporter spec --compilers js:babel-core/register --recursive",
    "test:watch": "npm test -- --watch",
    "storybook": "start-storybook -p 9001 -s ./public"
  },
  "license": "MIT",
  "bugs": "https://github.com/pmpkin/react-typeaheadix/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/pmpkin/react-typeaheadix.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "typeahead",
    "autocomplete",
    "react-component",
    "component"
  ],
  "devDependencies": {
    "@kadira/storybook": "^2.3.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.3.0",
    "fs": "0.0.2",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "path": "^0.12.7",
    "raw-loader": "^0.5.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-svg-loader": "^1.1.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2"
  },
  "peerDependencies": {
    "react": ">=0.14",
    "react-dom": ">=0.14"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  }
}
